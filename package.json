{
  "name": "custom-mui-library",
  "version": "0.1.0",
  "description": "Custom React component library with JSON-driven styling",
  "main": "dist/index.js",
  "files": [
    "dist",
    "src/styles/generated"
  ],
  "scripts": {
    "generate-css": "node src/scripts/generateThemeFiles.js src/styles/theme.json src/styles/generated",
    "prepare": "npm run generate-css && npm run build:lib",
    "start": "react-scripts start",
    "build": "react-scripts build",
    "build:lib": "babel src/lib.js src/Components src/utils --out-dir dist --copy-files --config-file ./.babelrc",
    "test": "react-scripts test",
    "eject": "react-scripts eject"
  },
  "keywords": [
    "react",
    "component",
    "library",
    "css",
    "json"
  ],
  "author": "",
  "license": "MIT",
  "dependencies": {
    "@emotion/react": "^11.14.0",
    "@emotion/styled": "^11.14.0",
    "@mui/icons-material": "^7.0.2",
    "@mui/material": "^7.0.2",
    "@vitejs/plugin-react": "^4.4.0",
    "prop-types": "^15.8.1",
    "react-router-dom": "^7.5.0",
    "react-scripts": "^5.0.1",
    "vite": "^6.3.1"
  },
  "peerDependencies": {
    "react": "^17.0.0 || ^18.0.0",
    "react-dom": "^17.0.0 || ^18.0.0"
  },
  "devDependencies": {
    "@babel/cli": "^7.27.2",
    "@babel/core": "^7.27.1",
    "@babel/preset-env": "^7.27.2",
    "@babel/preset-react": "^7.27.1",
    "@rollup/plugin-babel": "^6.0.3",
    "@rollup/plugin-commonjs": "^25.0.2",
    "@rollup/plugin-node-resolve": "^15.1.0",
    "@rollup/plugin-terser": "^0.4.3",
    "@testing-library/jest-dom": "^5.16.5",
    "@testing-library/react": "^14.0.0",
    "babel-loader": "^10.0.0",
    "cross-env": "^7.0.3",
    "css-loader": "^7.1.2",
    "html-webpack-plugin": "^5.6.3",
    "jest": "^29.5.0",
    "rollup": "^3.25.1",
    "rollup-plugin-peer-deps-external": "^2.2.4",
    "rollup-plugin-postcss": "^4.0.2",
    "style-loader": "^4.0.0",
    "webpack": "^5.99.8",
    "webpack-cli": "^6.0.1",
    "webpack-dev-server": "^5.2.1"
  }
}
