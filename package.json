{
  "name": "custom-mui-library",
  "version": "0.1.0",
  "description": "Custom React component library with JSON-driven styling",
  "type": "module",
  "main": "dist/index.js",
  "files": [
    "dist",
    "src/styles/theme-files"
  ],
  "scripts": {
    "generate-css": "node src/utils/convert-theme-tokens.js",
    "prepare": "npm run generate-css",
    "build:lib": "echo 'Build lib script placeholder'",
    "convert-tokens": "node utils/convert-theme-tokens.js",
    "start": "webpack serve --config webpack.config.mjs",
    "build": "NODE_ENV=production webpack --config webpack.config.mjs",
    "build-preview": "NODE_ENV=production webpack --config webpack.preview.config.js",
    "dev-preview": "webpack serve --config webpack.preview.config.js --mode development --port 3001",
    "test": "echo \"Error: no test specified\" && exit 1",
    "eject": "react-scripts eject"
  },
  "keywords": [
    "react",
    "component",
    "library",
    "css",
    "json",
    "figma",
    "design-system"
  ],
  "author": "",
  "license": "MIT",
  "homepage": "https://lwnoble.github.io/mui-custom-library/",
  "repository": {
    "type": "git",
    "url": "https://github.com/lwnoble/mui-custom-library.git"
  },
  "dependencies": {
    "@emotion/react": "^11.14.0",
    "@emotion/styled": "^11.14.0",
    "@mui/icons-material": "^7.0.2",
    "@mui/material": "^7.0.2",
    "prop-types": "^15.8.1",
    "react": "^18.3.1",
    "react-dom": "^18.3.1"
  },
  "peerDependencies": {
    "react": "^18.3.1",
    "react-dom": "^18.3.1"
  },
  "devDependencies": {
    "@babel/cli": "^7.21.0",
    "@babel/core": "^7.27.1",
    "@babel/preset-env": "^7.27.2",
    "@babel/preset-react": "^7.27.1",
    "@babel/preset-typescript": "^7.27.1",
    "babel-loader": "^10.0.0",
    "css-loader": "^7.1.2",
    "html-webpack-plugin": "^5.6.3",
    "style-loader": "^4.0.0",
    "webpack": "^5.95.0",
    "webpack-cli": "^5.1.4",
    "webpack-dev-server": "^5.1.0"
  }
}